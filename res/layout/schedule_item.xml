<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scheduleListView"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/schedule"
        android:layout_marginStart="47dp"
        android:layout_alignBaseline="@+id/teamName"
        android:layout_alignBottom="@+id/teamName"
        android:layout_toEndOf="@+id/teamName" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/teamName"
        android:layout_marginStart="23dp"
        android:layout_marginBottom="12dp"
        android:layout_alignBottom="@+id/teamLogo"
        android:layout_toEndOf="@+id/teamLogo" />

    <ImageView
        android:id="@+id/teamLogo"
        android:layout_marginLeft="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:src="@mipmap/osu"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>